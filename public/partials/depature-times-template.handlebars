{{#routes}}
  <div class="{{#if absoluteTimes}} arrivalRouteName {{/if}}" 
      style="display:block;vertical-align: top;"
        data-arrival-route-name="{{routeName}}"
        data-arrival-times="{{arrivalTimes}}"
        data-arrival-absolute-times="{{absoluteTimes}}">
        <span style="">{{routeName}}: </span>
        <span style="text-align:right">{{arrivalTimes}}</span>
  </div>
{{/routes}}